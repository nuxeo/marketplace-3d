<package type="addon" name="nuxeo-platform-3d" version="@VERSION@">
    <title>Nuxeo Platform 3D</title>
    <description>The Nuxeo Platform 3D addon provides support for 3D file types: canonical views extraction, technical metadata, transformations and 3D previewer for web applications. Those new capabilities can be used for creative process in the textile industry for instance.</description>
    <home-page>https://doc.nuxeo.com/nxdoc/nuxeo-3d/</home-page>
    <vendor>Nuxeo</vendor>
    <installer restart="true" />
    <uninstaller restart="true" />
    <hotreload-support>true</hotreload-support>
    <require-terms-and-conditions-acceptance>true</require-terms-and-conditions-acceptance>
    <nuxeo-validation>nuxeo_certified</nuxeo-validation>
    <production-state>production_ready</production-state>
    <supported>true</supported>
    <platforms>
        <platform>cap-@TARGET_VERSION@</platform>
        <platform>cap-@TARGET_VERSION@-HF*</platform>
        <platform>server-@TARGET_VERSION@</platform>
        <platform>server-@TARGET_VERSION@-HF*</platform>
    </platforms>
    <license>AL 2.0</license>
    <license-url>http://www.apache.org/licenses/LICENSE-2.0.html</license-url>
    <visibility>PUBLIC</visibility>
</package>
